---
    /**
    * VSL 1.0.0
    */

    //
    import BaseLayout from './_layouts/BaseLayout.astro';

    // Modal
    import Header from '../components/all/Header.astro';
    import OffCanvasOverlayMenu from '../components/all/OffCanvasOverlayMenu.astro';
    import Footer from '../components/all/Footer.astro';

    import { COMPANY } from "../company";

    const config = {
        meta: {
            title: "BioLuna",
            description: "BioLuna",
            keywords: "BioLuna, Sun Coast Sciences",
        },        
        modal: {
            privacyPolicy: true,
            contactUs: true,
            termsConditions: true,
            processing: false
        },
        company: COMPANY
    }

    import '../css/bootstrap.min.css';
    import '../css/font-awesome.min.css';
    import '../css/flaticon.css';
    import '../css/owl.carousel.min.css';
    import '../css/barfiller.css';
    import '../css/magnific-popup.css';
    import '../css/slicknav.min.css';
    import '../css/style.css';

    import { Image } from 'astro:assets';
    import banner_bg from '../images/banner-bg.jpg';
    import hero_1 from '../images/hero/hero-1.jpg';
    import hero_2 from '../images/hero/hero-2.jpg';

    import testimonial_1 from '../images/testimonial/testimonial-1.jpg';
    import testimonial_2 from '../images/testimonial/testimonial-2.jpg';
    import testimonial_3 from '../images/testimonial/testimonial-3.jpg';

              
---

<BaseLayout meta={config.meta} company={ config.company }>
    <!-- Modal -->
    <!-- <Modal modals={config.modal} funnel={ config.funnel } /> -->
    <!-- END Modal -->

    <!-- Page Preloder -->
    <div id="preloder">
        <div class="loader"></div>
    </div>

    <OffCanvasOverlayMenu company={config.company} />

    <Header company={config.company} page="home" />

    <!-- Hero Section Begin -->
    <section class="hero-section">
        <div class="hs-slider owl-carousel">
            <div class="hs-item set-bg" data-setbg={hero_1.src}>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 offset-lg-6">
                            <div class="hi-text">
                                <h1>Get <strong>Stronger</strong>, Leaner, and Fitter</h1>
                                <span>Without Wasting Time</span>
                                <a href="#" class="primary-btn">Book a Free Consultation</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="hs-item set-bg" data-setbg={hero_2.src}>
                <div class="container">
                    <div class="row">
                        <div class="col-lg-6 offset-lg-6">
                            <div class="hi-text">
                                <h1>Get <strong>Stronger</strong>, Leaner, and Fitter</h1>
                                <span>Without Wasting Time</span>
                                <a href="#" class="primary-btn">Book a Free Consultation</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Hero Section End -->

    <!-- ChoseUs Section Begin -->
    <section class="choseus-section spad">
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <div class="section-title">
                        <span>Why chose me?</span>
                        <h2>THE PROCESS</h2>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-lg-3 col-sm-6">
                    <div class="cs-item">
                        <span class="flaticon-034-stationary-bike"></span>
                        <h4>Assessment & Program Design</h4>
                        <p>Create a personalized training plan that fits your goals, abilities, and limitations.</p>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="cs-item">
                        <span class="flaticon-033-juice"></span>
                        <h4>Nutrition Strategy</h4>
                        <p>Provide a realistic, sustainable nutrition plan based on your needs and goals.</p>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="cs-item">
                        <span class="flaticon-002-dumbell"></span>
                        <h4>Weekly Check-Ins & Accountability</h4>
                        <p>Track progress, reflect, and make adjustments as needed.</p>
                    </div>
                </div>
                <div class="col-lg-3 col-sm-6">
                    <div class="cs-item">
                        <span class="flaticon-014-heart-beat"></span>
                        <h4>Lifestyle & Long-Term Success</h4>
                        <p>Teach you to integrate fitness into your lives naturally, balancing work, weekends, and vacations, training efficiently, and building healthy habits for sustainable results.</p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- ChoseUs Section End -->

    <!-- Gallery Section Begin -->
